@import "bootstrap-sprockets";
@import "bootstrap";

body {
  background-color: gray;
}

.masthead {
  min-height: 125px;
}

.homepage-content-wrapper {
    padding-left: 0;
    padding-right: 0;
    padding-bottom: 0 !important;
    background: gray;
}

.homepage-content-wrapper figure.hero-image {
    position: relative;
    padding-left: 0;
    padding-right: 0
}

.homepage-content-wrapper figure.hero-image img.img-responsive {
    background-size: auto cover;
    border: none;
    left: 0px;
}

.homepage-content-wrapper figure.hero-image figcaption {
    position: relative;
    bottom: -745px;
    left: 545px;
    background: rgba(0,0,0,0.25);
    font-weight: 300;
    font-style: italic
}

.homepage-content-wrapper figure.hero-image figcaption a,.homepage-content-wrapper figure.hero-image figcaption a:link,.homepage-content-wrapper figure.hero-image figcaption a:active,.homepage-content-wrapper figure.hero-image figcaption a:visited,.homepage-content-wrapper figure.hero-image figcaption a:hover {
    color: white;
    text-decoration: none;
    opacity: 1
}

.homepage-content-wrapper .digital-description .pre-heading {
    color: #646469;
    margin-bottom: 3px;
    margin-top: 12px;
    width: 100%;
    left: 875px;
    bottom: -80px;
    color: black;
}

.homepage-content-wrapper .digital-description h2,.homepage-content-wrapper .digital-description .modal-title {
    margin-top: 0;
    left: 646px;
}

.homepage-content-wrapper .digital-description-2 .pre-heading {
    color: #646469;
    margin-bottom: 3px;
    margin-top: 12px;
    width: 100%;
    left: 875px;
    bottom: -80px;
    color: black;
}

.homepage-content-wrapper .digital-description-2 h2,.homepage-content-wrapper .digital-description .modal-title {
    margin-top: 0;
    left: 646px;
}

.homepage-content-wrapper .chf-callout-button {
    width: 100%;
    text-transform: uppercase;
    border-radius: 0;
    letter-spacing: 0.04em;
    margin-bottom: 22px;
    white-space: initial;
    border-radius: 2px
}

.homepage-content-wrapper .search-container {
    padding-left: 0px;
    padding-right: 0px
}

@media (min-width: 992px) {
    .homepage-content-wrapper .home-right-column {
        height:calc(((600vw / 12) - 20px) / 1.366);
        justify-content: center
    }

    .homepage-content-wrapper .home-right-column .search-container {
        max-width: 25em;
        min-height: 100%;
        margin-right: 10px;
    }

    .homepage-content-wrapper .home-right-column .search-container .pre-heading {
        margin-top: 0
    }
}

.homepage-content-wrapper .search-callouts .input-group {
    min-height: 46px;
    float: right;
    width: 25;
}

.homepage-content-wrapper .search-callouts .input-group input.q,.homepage-content-wrapper .search-callouts .input-group button {
    min-height: 46px
}

@media (min-width: 768px) and (max-width: 991px) {
    .homepage-content-wrapper .search-callouts {
        margin-top:13px
    }
}

.homepage-content-wrapper .categories-title-div {
    padding-left: 15px;
    background-color: #646469
}

.homepage-content-wrapper .categories-title-div h2,.homepage-content-wrapper .categories-title-div .modal-title {
    margin-bottom: 0px
}

.homepage-content-wrapper .categories-list {
    background-color: #646469;
    padding-top: 34px;
    justify-content: center
}

.homepage-content-wrapper .categories-list .collection {
    position: relative;
    width: 350px;
    min-width: 350px;
    height: 350px;
    margin: 59px
}

@media (max-width: 350px) {
    .homepage-content-wrapper .categories-list .collection {
        width:100vw;
        min-width: auto;
        height: 100vw;
        margin: 59px 0
    }
}

.homepage-content-wrapper .categories-list .collection .collection-title-wrapper {
    position: absolute;
    bottom: 0;
    left: 0;
    width: 100%;
    height: 56px;
    color: #050939;
    background-color: #a6e5d8;
    padding: 10px
}

.homepage-content-wrapper .categories-list .collection .collection-title-wrapper .collection-title {
    text-align: center;
    font-weight: 600;
    text-transform: uppercase;
    font-size: 18px;
    padding-top: 5px;
    padding-bottom: 5px
}

.homepage-content-wrapper .featured-collection-callouts {
    padding-left: 5px;
    padding-right: 5px
}

@media (min-width: 480px) and (max-width: 767px) {
    .homepage-content-wrapper .featured-collection-callouts {
        margin-top:30px
    }
}

@media (min-width: 768px) {
    .homepage-content-wrapper .featured-collection-container {
        background-color:white;
        align-items: center
    }

    .homepage-content-wrapper .featured-collection-container .featured-collection-callouts {
        margin-left: 10%;
        margin-right: 2%
    }

    .homepage-content-wrapper .featured-collection-container .featured-collection-callouts h2,.homepage-content-wrapper .featured-collection-container .featured-collection-callouts .modal-title {
        margin-top: 0
    }
}

.homepage-content-wrapper .featured-collection-image {
    padding: 0
}

.homepage-content-wrapper .featured-collection-image .featured-collection-matte {
    position: relative
}

.homepage-content-wrapper .featured-collection-image .featured-collection-matte img {
    width: 100%
}

.homepage-content-wrapper .featured-collection-image .featured-collection-matte figcaption {
    position: absolute;
    bottom: 0;
    right: 0;
    background: rgba(0,0,0,0.25);
    padding: 5px 10px;
    font-weight: 300;
    font-style: italic
}

.homepage-content-wrapper .featured-collection-image .featured-collection-matte figcaption a {
    color: white;
    text-decoration: none;
    opacity: 1
}


